<body>
  <div class="hero_area">
    <!-- header section strats -->
    <jsp:include page="taskbar.jsp"></jsp:include>
    <!-- end header section -->

  </div>
  <!-- end hero area -->

  <!-- contact section -->

  <section class="contact_section layout_padding">
    <div class="container px-0">
      <div class="heading_container ">
        <h2 class="">
          Profile
        </h2>
      </div>
    </div>
    <div class="container container-bg">
      <div class="row">
        <div class="col-lg-7 col-md-6 px-0">
          <div class="map_container">
            <div class="map-responsive">
                <a href="orderline">
                <image src="images/istockphoto-1440245759-170667a.jpg" alt="">
                <h5 style="text-align: center">in transit</h5>
            </a>
            </div>
          </div>
        </div>
        <div class="col-md-6 col-lg-5 px-0">
          <form action="#">
            <div>
                <input value="${sessionScope.acc.name}"type="text" placeholder="Name" />
            </div>
            <div>
              <input value="${sessionScope.acc.address}" type="email" placeholder="Address" />
            </div>
            <div>
              <input value="${sessionScope.acc.phone}" type="text" placeholder="Phone" />
            </div>
            <div>
              <input type="text" class="message-box" placeholder="Message" />
            </div>
            <div class="d-flex ">
              <button>
                Update
              </button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </section>

  <!-- end contact section -->

  <!-- info section -->

  <jsp:include page="footer.jsp"></jsp:include>
    <!-- footer section -->

  </section>

  <!-- end info section -->


